{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "comma-dangle": 0,
        "prefer-const": 0,
        "space-before-function-paren": 0,
        "indent": [2, 4],
        "camelcase": 2,
        "max-len": [2, 130]
    }
}
